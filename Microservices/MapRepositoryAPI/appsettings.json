{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      },
      "Using": [ "Serilog.Sinks.Seq" ]
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seqlogger:5341"
        }
      }
    ]
  },
  "MinIoConfiguration": {
    "endpoint": "minio:9000",
    "bucketName": "amielexamplebucket"
  },
  "AllowedHosts": "*"
}
