{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Debug"
      },
      "Using": [ "Serilog.Sinks.Seq" ]
    },
    "Properties": {
      "Application": "NotificationService"
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seqlogger:5341"
        }
      }
    ]
  },
  "Settings": {
    "NotifictionWsEndpoint": "notification",
    "HostName": "rabbithost",
    "TopicName": "entityQueue"
  },
  "AllowedHosts": "*"
}
